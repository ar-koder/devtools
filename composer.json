{
  "name": "arnaud-ritti/devtools",
  "description": "An AIO devtools service made with Symfony",
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "RC",
  "prefer-stable": true,
  "require": {
    "php": ">=8.0",
    "ext-ctype": "*",
    "ext-gd": "*",
    "ext-iconv": "*",
    "api-platform/core": "^3.3",
    "doctrine/annotations": "^1.0",
    "doctrine/doctrine-bundle": "^2.6",
    "doctrine/doctrine-migrations-bundle": "^3.2",
    "doctrine/orm": "^2.11",
    "easycorp/easyadmin-bundle": "^4.0",
    "knplabs/knp-menu-bundle": "^3.2",
    "nelmio/cors-bundle": "^2.2",
    "nicoverbruggen/image-generator": "^2.1",
    "phpdocumentor/reflection-docblock": "^5.3",
    "phpstan/phpdoc-parser": "^1.4",
    "sensio/framework-extra-bundle": "^6.1",
    "sensiolabs/ansi-to-html": "^1.2",
    "symfony/apache-pack": "^1.0",
    "symfony/asset": "6.4.*",
    "symfony/console": "6.4.*",
    "symfony/doctrine-messenger": "6.4.*",
    "symfony/dotenv": "6.4.*",
    "symfony/expression-language": "6.4.*",
    "symfony/flex": "^2",
    "symfony/form": "6.4.*",
    "symfony/framework-bundle": "6.4.*",
    "symfony/http-client": "6.4.*",
    "symfony/intl": "6.4.*",
    "symfony/mailer": "6.4.*",
    "symfony/mime": "6.4.*",
    "symfony/monolog-bundle": "^3.0",
    "symfony/notifier": "6.4.*",
    "symfony/process": "6.4.*",
    "symfony/property-access": "6.4.*",
    "symfony/property-info": "6.4.*",
    "symfony/proxy-manager-bridge": "6.4.*",
    "symfony/runtime": "6.4.*",
    "symfony/security-bundle": "6.4.*",
    "symfony/serializer": "6.4.*",
    "symfony/string": "6.4.*",
    "symfony/translation": "6.4.*",
    "symfony/twig-bundle": "6.4.*",
    "symfony/uid": "6.4.*",
    "symfony/ux-live-component": "^2.1",
    "symfony/ux-twig-component": "^2.1",
    "symfony/validator": "6.4.*",
    "symfony/web-link": "6.4.*",
    "symfony/webapp-meta": "^1.0",
    "symfony/webpack-encore-bundle": "^1.14",
    "symfony/workflow": "6.4.*",
    "symfony/yaml": "6.4.*",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^2.12|^3.0",
    "webonyx/graphql-php": "^14.11",
    "zenstruck/schedule-bundle": "^1.2"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "symfony/flex": true,
      "symfony/runtime": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    },
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "6.4.*"
    }
  },
  "require-dev": {
    "deployer/deployer": "^7",
    "doctrine/doctrine-fixtures-bundle": "^3.4",
    "fzaninotto/faker": "^1.5",
    "justinrainbow/json-schema": "^5.2",
    "nunomaduro/phpinsights": "^2.2",
    "phpunit/phpunit": "^9.5",
    "rector/rector": "^0.13.0",
    "symfony/browser-kit": "6.4.*",
    "symfony/css-selector": "6.4.*",
    "symfony/debug-bundle": "6.4.*",
    "symfony/maker-bundle": "^1.0",
    "symfony/phpunit-bridge": "^6.0",
    "symfony/stopwatch": "6.4.*",
    "symfony/web-profiler-bundle": "6.4.*"
  }
}