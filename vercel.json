{
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.1"
        },
        "public/**": {
            "runtime": "@vercel/static"
        }
    },
    "routes": [
        {
            "src": "/(css|js)/(.*)",
            "dest": "public/$1/$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}